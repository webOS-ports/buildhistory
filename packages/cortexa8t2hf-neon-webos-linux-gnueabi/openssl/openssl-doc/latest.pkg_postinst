set -e
	# only update manual page index caches when manual files are built and installed
	if false; then
		if test -n "$D"; then
			if true; then
				$INTERCEPT_DIR/postinst_intercept update_mandb openssl-doc mlprefix= binprefix= bindir=/usr/bin sysconfdir=/etc mandir=/usr/share/man
			else
				$INTERCEPT_DIR/postinst_intercept delay_to_first_boot openssl-doc mlprefix=
			fi
		else
			mandb -q
		fi
	fi
