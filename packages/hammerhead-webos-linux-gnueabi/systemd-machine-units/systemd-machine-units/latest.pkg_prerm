#!/bin/sh
set -e
if type systemctl >/dev/null 2>/dev/null; then
	if [ -z "$D" ]; then
		systemctl stop wifi-macaddr-persister.service wifi-module-load.service hciattach.service dev-ttyHS99.device

		systemctl disable wifi-macaddr-persister.service wifi-module-load.service hciattach.service dev-ttyHS99.device
	fi
fi
