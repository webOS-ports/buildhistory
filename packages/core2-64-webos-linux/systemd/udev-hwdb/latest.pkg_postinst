set -e
	if test -n "$D"; then
		if true; then
			PSEUDO_UNLOAD=1 qemu-x86_64 -r 3.2.0  -cpu core2duo -L $D -E LD_LIBRARY_PATH=$D/usr/lib:$D/lib $D/bin/udevadm hwdb --update \
				--root $D
			chown root:root $D/etc/udev/hwdb.bin
		else
			$INTERCEPT_DIR/postinst_intercept delay_to_first_boot ${PKG} mlprefix=
		fi
	else
		udevadm hwdb --update
	fi
