#!/bin/sh\nset -e\nOPTS=""\n\nif [ -n "$D" ]; then\n    OPTS="--root=$D"\nfi\n\nif type systemctl >/dev/null 2>/dev/null; then\n\tif [ -z "$D" ]; then\n\t\tsystemctl stop anbox-container-manager.service anbox-session-manager.service\n\tfi\n\n\tsystemctl $OPTS disable anbox-container-manager.service anbox-session-manager.service\nfi\n