#!/bin/sh
set -e
if type systemctl >/dev/null 2>/dev/null; then
	if [ -z "$D" ]; then
		systemctl stop db8.service db8-maindb.service db8-mediadb.service db8-pre-config.service db8-tempdb.service

		systemctl disable db8.service db8-maindb.service db8-mediadb.service db8-pre-config.service db8-tempdb.service
	fi
fi
