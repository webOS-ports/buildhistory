set -e
set +e
if [ -n "$D" ]; then
    ln -sf uImage-5.10.31-v7l $D/boot/uImage > /dev/null 2>&1
else
    ln -sf uImage-5.10.31-v7l boot/uImage > /dev/null 2>&1
    if [ $? -ne 0 ]; then
        echo "Filesystem on boot/ doesn't support symlinks, falling back to copied image (uImage)."
        install -m 0644 boot/uImage-5.10.31-v7l boot/uImage
    fi
fi
set -e
