set -e
set +e
if [ -n "$D" ]; then
    ln -sf zImage-dtb-3.4.0 $D/boot/zImage-dtb > /dev/null 2>&1
else
    ln -sf zImage-dtb-3.4.0 boot/zImage-dtb > /dev/null 2>&1
    if [ $? -ne 0 ]; then
        echo "Filesystem on boot/ doesn't support symlinks, falling back to copied image (zImage-dtb)."
        install -m 0644 boot/zImage-dtb-3.4.0 boot/zImage-dtb
    fi
fi
set -e
